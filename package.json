{
  "name": "vue-zero-admin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p \"build-only --mode production\" --",
    "build:test": "run-p \"build-only --mode test\" --",
    "build:dev": "run-p \"build-only --mode development\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "npx oxlint && npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^12.8.2",
    "axios": "^1.8.4",
    "element-plus": "^2.9.6",
    "es-toolkit": "^1.33.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.6",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@intlify/core-base": "^11.1.2",
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@playwright/test": "^1.51.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@unocss/eslint-config": "^0.65.4",
    "@unocss/preset-rem-to-px": "^0.65.4",
    "@unocss/transformer-directives": "^0.65.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.15.15",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.15",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "unocss": "^0.65.4",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-oxlint": "^1.2.3",
    "vite-plugin-pages": "^0.32.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  },
  "types": "./types/index.d.ts",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
